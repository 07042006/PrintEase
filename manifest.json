{
    "manifest_version": 2,
    "name": "PrintEase: Extensão Automatizada de Impressão no Chrome",
    "version": "1.0",
    "permissions": [
      "downloads",
      "printerProvider"
    ],
    "options_page": "options.html",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "popup.html"
    }
  }
  